package weightmatrix2;

/**
 *
 * @author mitits
 */
/*
 * This class stores the link belongs to a sensor
 */
public class Sensor {
	private String link;

	Sensor() {
		this.link = null;
	}

	Sensor(String link) {
		this.link = link;
	}

	public String getLink() {
		return this.link;
	}
}
